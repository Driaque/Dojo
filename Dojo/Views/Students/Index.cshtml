@model IEnumerable<Dojo.Models.Student>

@{
    ViewBag.Title = "Students";
}

<div class="box box-info">

    <div class="box-header with-border">
        <h2>@ViewBag.Title</h2>
    </div>

    <p>
        @Html.ActionLink("Create New", "Create")
    </p>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.STUDENT_Fname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.STUDENT_Lname)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PhoneNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DateJoined)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsActive)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsParent)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Parent_ID)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Membership.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Rank.Title)
            </th>
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.STUDENT_Fname)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.STUDENT_Lname)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PhoneNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.DateJoined)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IsActive)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IsParent)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Parent_ID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Membership.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Rank.Title)
                </td>
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.STUDENT_ID }) |
                    @Html.ActionLink("Details", "Details", new { id = item.STUDENT_ID })

                </td>
            </tr>
        }

    </table>
    </div>